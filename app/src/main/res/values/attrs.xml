<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="md_selector" format="reference" />
    <attr name="md_btn_selector" format="reference" />
    <attr name="md_divider" format="color" />

    <attr name="md_dark_theme" format="boolean" />
    <attr name="md_title_color" format="color" />
    <attr name="md_content_color" format="color" />
    <attr name="md_accent_color" format="color" />
    <attr name="md_item_color" format="color" />
    <attr name="md_icon" format="reference" />


    <attr name="CustomFontFamily">
        <enum name="Light" value="1"></enum>
        <enum name="Regular" value="2"></enum>
        <enum name="Medium" value="3"></enum>
        <enum name="Bold" value="4"></enum>
        <enum name="MYR_PRO" value="5"></enum>
    </attr>

    <declare-styleable name="CustomTextView">
        <attr name="CustomFontFamily" />
    </declare-styleable>
    <declare-styleable name="PullToRefresh">
        <attr name="adapterViewBackground" format="reference|color" />
        <attr name="headerBackground" format="reference|color" />
        <attr name="headerTextColor" format="color" />
        <attr name="mode">
            <flag name="pullDownFromTop" value="0x1" />
            <flag name="pullUpFromBottom" value="0x2" />
            <flag name="both" value="0x3" />
        </attr>
        
       
        
    </declare-styleable>
    
    <declare-styleable name="SwipeRevealLayout">
        <attr name="dragEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
            <flag name="top" value="4" />
            <flag name="bottom" value="8" />
        </attr>
        
        <attr name="_mode">
            <enum name="normal" value="0" />
            <enum name="same_level"   value="1" />
        </attr>

        <attr name="flingVelocity" format="integer" />

        <attr name="minDistRequestDisallowParent" format="dimension" />
    </declare-styleable>
    
    
    
   <declare-styleable name="ShSwitchView">
        <attr name="tintColor" format="reference|color" />
        <attr name="outerStrokeWidth" format="reference|dimension" />
        <attr name="shadowSpace" format="reference|dimension" />
    </declare-styleable>
    
   
   
   <declare-styleable name="CircularSlider">
      
       
        <attr name="border_thickness" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="thumb_size" format="dimension" />
        <attr name="start_thumb_size" format="dimension" />
        <attr name="end_thumb_size" format="dimension" />
        <attr name="start_thumb_image" format="reference" />
        <attr name="end_thumb_image" format="reference" />
        <attr name="start_thumb_color" format="color" />
        <attr name="end_thumb_color" format="color" />
        <attr name="arc_color" format="color" />
        <attr name="arc_dash_size" format="dimension" />
    </declare-styleable>
   
    <declare-styleable name="HoloCircleSeekBar">
        <attr name="wheel_size" format="integer" />
        <attr name="pointer_size" format="dimension" />
        <attr name="max" format="integer"/>
        <attr name="show_text" format="boolean"/>
        <attr name="start_angle" format="integer"/>
        <attr name="end_angle" format="integer"/>
        <attr name="text_size" format="dimension"/>
        <attr name="init_position" format="integer"/>
        <attr name="wheel_active_color" format="string"/>
        <attr name="wheel_unactive_color" format="string"/>
        <attr name="pointer_color" format="string"/>
        <attr name="pointer_halo_color" format="string"/>
        <attr name="text_color" format="string"/>
    </declare-styleable>
    
    
    
    
    <declare-styleable name="cs_CircularSeekBar">
        <attr name="cs_circle_style">
            <enum name="butt" value="0" />
            <enum name="round" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="cs_progress" format="integer"/>
        <attr name="cs_max" format="integer"/>
        <attr name="cs_negative_enabled" format="boolean"/>
        <attr name="cs_move_outside_circle" format="boolean"/>
        <attr name="cs_maintain_equal_circle" format="boolean"/>
        <attr name="cs_use_custom_radii" format="boolean"/>
        <attr name="cs_lock_enabled" format="boolean"/>
        <attr name="cs_circle_x_radius" format="dimension" />
        <attr name="cs_circle_y_radius" format="dimension" />
        <attr name="cs_circle_stroke_width" format="dimension" />
        <attr name="cs_disable_pointer" format="boolean"/>
        <attr name="cs_pointer_stroke_width" format="dimension" />
        <attr name="cs_pointer_halo_width" format="dimension" />
        <attr name="cs_pointer_halo_border_width" format="dimension"/>
        <attr name="cs_circle_fill" format="color"/>
        <attr name="cs_circle_color" format="color"/>
        <attr name="cs_circle_progress_color" format="color"/>
        <attr name="cs_pointer_color" format="color"/>
        <attr name="cs_pointer_halo_color" format="color"/>
        <attr name="cs_pointer_halo_color_ontouch" format="color"/>
        <attr name="cs_pointer_alpha_ontouch" format="integer"/>
        <attr name="cs_pointer_angle" format="float"/>
        <attr name="cs_start_angle" format="float"/>
        <attr name="cs_end_angle" format="float"/>
    </declare-styleable>
    
    
    
    
    
     <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
    
    
    
     <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    
    
   
</resources>