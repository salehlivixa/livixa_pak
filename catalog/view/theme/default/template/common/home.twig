            success: function (data) {
                if (lang_valuex == 'en') {
                    Swal.fire({
                        icon: 'success',
                        title: 'Our Customer support will reach you soon!',
                        showConfirmButton: true,
                    });
                } else {
                    Swal.fire({
                        icon: 'success',
                        title: 'ممثل خدمات العملاء لدينا سيتواصل معك قريبا',
                        showConfirmButton: true,
                    });
                }
                $("#name").val('');
                $("#last_name").val('');
                $("#email").val('');
                $("#phone").val('');
                $("#message").val('');
                $("#check_loader").removeClass('overlay');
